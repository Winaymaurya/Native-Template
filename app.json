{
  "expo": {
    "name": "SMSStudent",
    "slug": "SMS-Student",
    "version": "1.0.0",
    "newArchEnabled": true,
    "owner": "winaymaurya",
    "orientation": "portrait",
    "icon": "./assets/images/student.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/student.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/s.gif",
        "backgroundColor": "#ffffff"
      },
      "package": "com.winaymaurya.Student"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/student.png",
          "color": "#ffffff",
          "sounds": ["./assets/images/saudio.wav"],
          "mode": "production"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
   
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e078cf4e-87d2-4eea-be41-1d26e172dbd1"
      }
    }
  }
}
